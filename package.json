{
  "name": "react-mobile-datepicker-typescript",
  "version": "1.0.11",
  "types": "dist",
  "description": "react based date picker like native",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "files": ["dist/*", "dist/**/*"],
  "amdName": "reactMobileDatePickerTs",
  "minified:main": "dist/index.min.js",
  "repository": "jin60641/react-mobile-datepicker",
  "homepage": "https://github.com/jin60641/react-mobile-datepicker#readme",
  "keywords": [
    "react-component",
    "react-datepicker",
    "date",
    "datepicker",
    "react",
    "mobile",
    "calendar"
  ],
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "npm-run-all clean transpile typescript minify size",
    "transpile": "webpack --config ./webpack.build.config.js",
    "typescript": "tsc --emitDeclarationOnly",
    "minify": "uglifyjs $npm_package_main -cm -o $npm_package_minified_main --source-map \"root='${npm_package_main}.map',url='${npm_package_minified_main}.map'\"",
    "size": "size=$(gzip-size $npm_package_minified_main) && echo \"gzip size: $size\"",
    "lint": "eslint -c .eslintrc.js '{lib,examples}/**/*.{ts,tsx}'",
    "start": "webpack serve"
  },
  "author": "rainie",
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/node": "^17.0.22",
    "@types/react": "16.14.0",
    "@types/react-dom": "^16.9.16",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^2.0.7",
    "cssnext": "^1.8.4",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.1",
    "gzip-size-cli": "^2.1.0",
    "html-webpack-plugin": "^5.5.0",
    "npm-run-all": "^3.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "styled-components": "^5.3.5",
    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.3.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "uglify-js": "^3.1.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "peerDependencies": {
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  },
  "license": "MIT"
}
